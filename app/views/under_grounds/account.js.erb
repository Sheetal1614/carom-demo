<%if @account %>
  <%if params[:action_2_perform] == 'clear_logs' %>
    $('#account_card_<%= @account.id %>').replaceWith('<%=j(render(partial: 'under_grounds/account_card', locals: {account: @account})) %>');
  <%elsif params[:action_2_perform] == 'delete' %>
    $('#account_card_<%= @account.id %>').remove();
  <%end %>
<%else %>
alert("Unable to find the account.")
<%end %>
$('.flash_notice_container').replaceWith('<%=j(render(partial: 'tool_components/flash_notice')) %>');